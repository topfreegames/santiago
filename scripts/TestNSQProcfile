lookup: bash -c "./bin/nsqlookupd --tcp-address=0.0.0.0:7777 --http-address=0.0.0.0:7778
nsqd: bash -c "./bin/nsqd --broadcast-address=localhost --lookupd-tcp-address=127.0.0.1:7777 -data-path /tmp/nsqd/1 --tcp-address=0.0.0.0:7779 --http-address=0.0.0.0:7780 --mem-queue-size=0 --worker-id=1"
admin: bash -c "./bin/nsqadmin --lookupd-http-address=127.0.0.1:7778 --http-address=0.0.0.0:7000"
