lookup: nsqlookupd --broadcast-address=localhost --tcp-address=0.0.0.0:6666 --http-address=0.0.0.0:6667 --broadcast-address=$MY_IP
nsqd: bash -c "echo 'Using IP as $MY_IP...' && nsqd --broadcast-address=localhost --lookupd-tcp-address=127.0.0.1:6666 -data-path /tmp/nsqd/1 --tcp-address=0.0.0.0:6668 --http-address=0.0.0.0:6669 --mem-queue-size=0 --worker-id=1 --broadcast-address=$MY_IP"
#nsqd2: nsqd --broadcast-address=localhost --lookupd-tcp-address=127.0.0.1:6666 -data-path /tmp/nsqd/2 --tcp-address=127.0.0.1:6670 --http-address=127.0.0.1:6671 --mem-queue-size=0 --worker-id=2
#nsqd3: nsqd --broadcast-address=localhost --lookupd-tcp-address=127.0.0.1:6666 -data-path /tmp/nsqd/3 --tcp-address=127.0.0.1:6672 --http-address=127.0.0.1:6673 --mem-queue-size=0 --worker-id=3
admin: nsqadmin --lookupd-http-address=127.0.0.1:6667 --http-address=0.0.0.0:7000
